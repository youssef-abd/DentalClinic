{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "skipLibCheck": true,
    "types": [
      "expo-router/types"
    ],
    "useUnknownInCatchVariables": false,
    "paths": {
      "@/*": ["./*"],
      "@lib/*": ["lib/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "app-example/**"
  ]
}
